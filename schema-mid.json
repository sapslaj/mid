        "stdin": { "type": "string" }
      },
      "type": "object",
      "required": ["command"]
    },
    "mid:types:TriggersInput": {
      "properties": {
        "refresh": { "type": "array", "items": { "$ref": "pulumi.json#/Any" } },
        "replace": { "type": "array", "items": { "$ref": "pulumi.json#/Any" }, "replaceOnChanges": true }
      },
      "type": "object"
    },
    "mid:types:TriggersOutput": {
      "properties": {
        "lastChanged": { "type": "string" },
        "refresh": { "type": "array", "items": { "$ref": "pulumi.json#/Any" } },
        "replace": { "type": "array", "items": { "$ref": "pulumi.json#/Any" } }
      },
